<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.allokgames.pokemongame">

    <application
        android:orientation="vertical"
        android:allowBackup="true"
        android:icon="@drawable/imgg"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Menu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".GAME_OVER" />
        <activity android:name=".Items" />
        <activity android:name=".Buy" />
        <activity android:name=".Stats" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".Gameplay" />

        <service
            android:name=".Upload"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".Notifyer"
            android:enabled="true"
            android:exported="true"></service><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>